@page "/"
@using EmptyBlazorApp1.Helpers;
@{
    List<string> names = new();
    names = TextHelpers.GetRandomNames(10);
}

<h1 class="text-4xl mb-6">Hello, world!</h1>
<div class="grid grid-cols-4 gap-4">
    @foreach (var name in names)
    {
        <div class="bg-gray-200 p-4">
            <p>@name</p>
        </div>
    }
</div>